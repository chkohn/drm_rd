          <item>&C_S_AXIS_VIDEO_FORMAT;</item>
          <item>&RAM_ADDR_BITS;</item>
         <item>&HYSTERESIS_LEVEL;</item>
        </group>
        <group id="Hidden">
          <display>Hidden</display>
          <item>&VID_OUT_DATA_WIDTH;</item>
          <item>&C_S_AXIS_VIDEO_TDATA_WIDTH;</item>
          <item>&FILL_GUARDBAND;</item>
          <item>&C_S_AXIS_VIDEO_PROTOCOL;</item>
        </group>
      </view>
</doc>
