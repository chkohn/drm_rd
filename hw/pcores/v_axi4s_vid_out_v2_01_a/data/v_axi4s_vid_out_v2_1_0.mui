<!DOCTYPE doc SYSTEM "../../ipdialog.dtd" [
  <!-- -->

  <!ENTITY C_S_AXIS_VIDEO_DATA_WIDTH '
  <widget id="C_S_AXIS_VIDEO_DATA_WIDTH">
    <key>C_S_AXIS_VIDEO_DATA_WIDTH</key>
    <label>Component Data Width</label>
    <tip>Specifies the number of bits in each pixel component</tip>
  </widget>
  '>
  <!ENTITY C_S_AXIS_VIDEO_FORMAT '
  <widget id="C_S_AXIS_VIDEO_FORMAT">
    <key>C_S_AXIS_VIDEO_FORMAT</key>
    <label>Video Format</label>
    <tip>Video Format. Used to determine the number of pixel components</tip>
  </widget>
  '>
  <!ENTITY VID_OUT_DATA_WIDTH '
  <widget id="VID_OUT_DATA_WIDTH">
    <key>VID_OUT_DATA_WIDTH</key>
    <label>Video Output Data Width</label>
    <tip></tip>
  </widget>
  '>
  <!ENTITY C_S_AXIS_VIDEO_TDATA_WIDTH '
  <widget id="C_S_AXIS_VIDEO_TDATA_WIDTH">
    <key>C_S_AXIS_VIDEO_TDATA_WIDTH</key>
    <label>AXI4-Stream TDATA Width</label>
    <tip></tip>
  </widget>
  '>
  <!ENTITY RAM_ADDR_BITS '
  <widget id="RAM_ADDR_BITS">
    <key>RAM_ADDR_BITS</key>
    <label>FIFO Depth</label>
    <tip>The number of locations in the FIFO</tip>
  </widget>
  '>
  <!ENTITY HYSTERESIS_LEVEL '
  <widget id="HYSTERESIS_LEVEL">
    <key>HYSTERESIS_LEVEL</key>
    <label>Hysteresis Level</label>
    <tip>The initial fill level of the FIFO. Must be less than FIFO Depth - 16.</tip>
  </widget>
  '>
  <!ENTITY FILL_GUARDBAND '
  <widget id="FILL_GUARDBAND">
    <key>FILL_GUARDBAND</key>
    <label>Fill Guardband</label>
    <tip></tip>
  </widget>
  '>
  <!ENTITY VTG_MASTER_SLAVE '
  <widget id="VTG_MASTER_SLAVE">
    <key>VTG_MASTER_SLAVE</key>
    <label>Master or Slave mode</label>
    <tip>Specifies whether the core acts as a timing master or a timing slave</tip>
  </widget>
  '>
  <!ENTITY C_S_AXIS_VIDEO_PROTOCOL '
  <widget id="C_S_AXIS_VIDEO_PROTOCOL">
    <key>C_S_AXIS_VIDEO_PROTOCOL</key>
    <label>AXI4-Stream Master Protocol</label>
    <tip>AXI4-Stream Master Protocol</tip>
        <value type="enum" default="XIL_AXI_STREAM_VID_DATA">
            <range>
                <discrete data="XIL_AXI_STREAM_VID_DATA" />
            </range>
        </value>
  </widget>
  '>
]>


